{
  "name": "six-cities",
  "version": "5.0.0",
  "description": "Проект «Шесть городов» от HTML Academy",
  "keywords": [
    "rest",
    "api",
    "node.js"
  ],
  "main": "main.js",
  "scripts": {
    "start": "npm run build && node ./dist/main.js",
    "build": "npm run clean && npm run compile",
    "lint": "eslint src/ --ext .ts",
    "compile": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "ts": "ts-node",
    "start:mock-api": "npx json-server ./mocks/mock-server-data.json --port 4000"
  },
  "devDependencies": {
    "@types/node": "18.17.17",
    "@types/pino": "^7.0.5",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "eslint": "8.49.0",
    "eslint-config-htmlacademy": "9.1.1",
    "pino-pretty": "^13.1.2",
    "rimraf": "5.0.1",
    "ts-node": "10.9.1",
    "tslib": "^2.8.1",
    "typescript": "5.2.2"
  },
  "type": "module",
  "engines": {
    "node": "^18.0.0",
    "npm": ">=8"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "chalk": "^5.6.2",
    "dotenv": "^17.2.3",
    "json-server": "^0.17.3",
    "pino": "^8.15.1",
    "reflect-metadata": "^0.2.2"
  }
}
